<script>
  //jquery wrapper
  $(function(){
    
    function initialize_geocomplete(){
      
      //wait until google library is loaded
      if(window.google == undefined) return setTimeout(initialize_geocomplete,1000);
      
      //initialize every dom control with attribute data-address
      $("[data-address]").geocomplete({
        map: "<%=map%>",
        location: "<%=location%>"
      });
      
    }
    
    initialize_geocomplete();
    
    
  });
</script>
    